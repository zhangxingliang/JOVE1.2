function GetClipStatus(status) {
    if (status == ClipStatus.ET_CLIP_CAPTURESTATUS_NORMAL) {
        return "Normal";
    }
    if (status == ClipStatus.ET_CLIP_CAPTURESTATUS_CAPTURE) {
        return "Capture";
